datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  connectedId   String @id
  password      String @db.Text()
  birthDate     String
  certFile      String @db.Text()
  userName      String
}

model Token {
  token_name    String @unique @id
  access_token  String @db.Text()
  scope         String
  token_type    String
  expires_in    Int
}